{
  "name": "birthday-messenger",
  "version": "1.0.0",
  "author": "Tony Dong",
  "license": "ISC",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --exit -r ts-node/register ./tests/*.test.ts",
    "lint": "tslint -c ./tslint.json './app/**/*.ts'",
    "build": "yarn run lint && tsc",
    "migrate": "knex --knexfile database/knexfile.js migrate:latest",
    "rollback": "knex --knexfile database/knexfile.js migrate:rollback",
    "make": "knex --knexfile database/knexfile.js migrate:make",
    "seed": "knex --knexfile database/knexfile.js seed:run",
    "job": "node -r dotenv/config ./lib/app/job.js",
    "job:birthdays": "node -r dotenv/config ./lib/app/index.js birthdays",
    "job:messages": "node -r dotenv/config ./lib/app/index.js messages"
  },
  "dependencies": {
    "aws-sdk": "^2.1194.0",
    "better-sqlite3": "^7.6.2",
    "knex": "^2.2.0",
    "moment": "^2.27.0",
    "nodemailer": "^6.7.8",
    "objection": "^3.0.1",
    "sqlite3": "^5.0.11"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/chai": "^4.2.12",
    "@types/faker": "^4.1.12",
    "@types/mocha": "^7.0.2",
    "@types/mock-knex": "^0.4.4",
    "@types/nock": "^11.1.0",
    "@types/node": "14.18.16",
    "@types/nodemailer": "6.4.5",
    "@types/sinon": "^7.5.2",
    "@types/superagent": "4.1.13",
    "@types/supertest": "^2.0.11",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "faker": "^6.6.6",
    "mocha": "^10.0.0",
    "mock-knex": "^0.4.11",
    "nock": "^13.2.9",
    "node-dev": "^7.4.3",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "supertest": "^6.2.4",
    "timekeeper": "^2.2.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tslint": "^6.1.3",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.7.4"
  }
}
